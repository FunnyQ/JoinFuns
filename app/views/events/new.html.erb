<div class="container">
<%= form_for @event ,:html => { :multipart => true } do |f|  %>

<div class="input-field">
<%= f.text_field :title  %>
<%= f.label :title, "title" %>
</div>

<div class="input-field">
<%= f.text_field :hoster %>
<%= f.label :hoster, "hoster" %>
</div>

<div class="input-field">
<%= f.text_field :address %>
<%= f.label :address, "address" %>
</div>

	<div class="row">
		<div class='col-md-6'>
			<div class="form-group">
				<div class='input-group date' >
					<%= f.label :start_time, "start_time" %>
					<%= f.text_field :end_time ,:class=>"form-control", :id=>'dateeee' %>
				
				</div>
			</div>
		</div>
	

		<div class='col-md-6'>
			<div class="form-group">
				<div class='input-group date' >
					<%= f.label :end_time, "end_time" %>
					<%= f.text_field :end_time ,:class=>"form-control", :id=>'dateeee2' %>
			
				</div>
			</div>
		</div>
	</div>


<div class="input-field">
	<%= f.text_area :description,:class=>"materialize-textarea" %>
	<%= f.label :description, "Description" %> 
</div>
   
 <%= f.fields_for :photos do |photo| %>  
    
      <%= photo.file_field :pic %>
  
 <% end %>

  

<%= f.submit "Create",:class=>"btn waves-light btn-large" %>



<% end %>
</div>

<%= content_for :handwrite_js do %>
 <script>
 $("#dateeee2").datetimepicker()
 $("#dateeee").datetimepicker()
        $("#dateeee").on("dp.change", function (e) {
            $('#dateeee2').data("DateTimePicker").minDate(e.date);
        });
        $("#dateeee2").on("dp.change", function (e) {
            $('#dateeee').data("DateTimePicker").maxDate(e.date);
        });
 </script>
<% end %>
 